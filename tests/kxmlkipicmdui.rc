<!DOCTYPE kpartgui SYSTEM "kpartgui.dtd">
<gui version="6" name="kxmlkipicmd" >

    <MenuBar>

        <Menu name="Browse" >
                <text>&amp;Browse</text>
        </Menu>

        <Menu name="Album" >
            <text>&amp;Album</text>
                <ActionList name="album_kipi_actions"/>
        </Menu>

        <Menu name="Image" >
            <text>&amp;Image</text>
                <ActionList name="image_kipi_actions"/>
        </Menu>

        <Menu name="Edit" >
            <text>&amp;Edit</text>
        </Menu>

        <Menu name="View" >
            <text>&amp;View</text>
        </Menu>

        <Menu name="Tools"><text>&amp;Tools</text>
            <ActionList name="tool_kipi_actions"/>
            <Separator/>
            <ActionList name="batch_kipi_actions"/>
        </Menu>

        <Menu name="Import"><text>I&amp;mport</text>
            <ActionList name="import_kipi_actions"/>
        </Menu>

        <Menu name="Export"><text>&amp;Export</text>
            <ActionList name="export_kipi_actions"/>
        </Menu>

        <Menu name="settings" noMerge="1"><Text>&amp;Settings</Text>
            <Merge name="StandardToolBarMenuHandler" />
            <Action name="options_show_menubar"/>
            <Separator/>
            <Action name="options_configure_keybinding"/>
            <Action name="options_configure_toolbars"/>
            <Action name="options_configure"/>
        </Menu>

        <Menu name="help"><Text>&amp;Help</Text>
            <Action name="help_kipi"/>
        </Menu>

        <Merge/>

    </MenuBar>

    <ToolBar noMerge="1" name="mainToolBar" >
        <text>Main Toolbar</text>
        <Action name="options_show_menubar" />
        <Action name="options_show_toolbar" />
        <Separator/>
        <Action name="options_configure_keybinding" />
        <Action name="options_configure_toolbars" />
        <Action name="options_configure" />
    </ToolBar>

    <MainWindow>
    </MainWindow>

    <ActionProperties/>

</gui>

